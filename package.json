{
  "name": "traveller",
  "version": "1.0.0",
  "description": "Traveller API",
  "projectPrefix": "traveller",
  "main": "index.js",
  "engines": {
    "npm": ">=6.0.0",
    "node": ">=14.0.0"
  },
  "scripts": {
    "postgres:up": "docker-compose -f docker.yml up",
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc",
    "start": "npm run typeorm:deploy:run && node ./dist/src/index.js",
    "dev": "ts-node-dev --transpile-only --respawn --inspect=9229 src/index.ts",
    "lint": "eslint . --ext ts",
    "lint:fix": "eslint . --ext ts --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts",
    "prepare": "husky install",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js --config ./src/database/ormconfig.ts",
    "typeorm:deploy": "node ./node_modules/typeorm/cli.js --config ./dist/src/database/ormconfig.js",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:migrate:create": "npm run typeorm migration:create -- -n",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm:deploy:run": "npm run typeorm:deploy migration:run",
    "typeorm:undo": "npm run typeorm migration:revert",
    "typeorm:drop": "npm run typeorm schema:drop",
    "typeorm:sync": "npm run typeorm schema:sync",
    "typeorm:test:sync": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.test ts-node -r dotenv/config ./node_modules/typeorm/cli.js --config ./src/database/ormconfig.ts schema:sync"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.7",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "commitlint-plugin-function-rules": "^1.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "flat": "^5.0.2",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.30",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.10",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "0.2.41",
    "winston": "^3.3.3",
    "yup": "^0.32.11"
  }
}
